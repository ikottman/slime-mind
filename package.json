{
    "name": "game",
    "version": "1.0",
    "description": "pixi.js typescript boilerplate",
    "scripts": {
        "start": "parcel ./src/main.html --port=8080 --open ",
        "build": "rm -rf ./dist && parcel build ./src/main.html --public-url ./",
        "pretty-format": "npx prettier --write src/**/*.ts"
    },
    "author": "ikottman",
    "license": "MIT",
    "dependencies": {
        "pixi.js": "^5.2.3"
    },
    "devDependencies": {
        "@types/jsdom": "^16.2.1",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "jsdom": "^16.2.2",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-static-files-copy": "^2.3.1",
        "prettier": "^2.0.5",
        "typescript": "^3.8.3"
    }
}
