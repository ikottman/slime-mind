{
    "name": "game",
    "version": "1.0",
    "description": "pixi.js typescript boilerplate",
    "scripts": {
        "start": "parcel ./src/index.html --port=8080 ",
        "build": "rm -rf ../blog/static/apps/slime-mind && parcel build ./src/index.html --public-url ./ --out-dir ../blog/static/apps/slime-mind && echo \"https://github.com/ikottman/game/commit/$(git rev-parse HEAD)\"",
        "pretty-format": "npx prettier --write src/**/*.ts"
    },
    "author": "ikottman",
    "license": "MIT",
    "dependencies": {
        "codejar": "^3.0.0",
        "highlight.js": "^10.0.3",
        "pixi.js": "^5.2.3"
    },
    "devDependencies": {
        "@types/jsdom": "^16.2.1",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "jsdom": "^16.2.2",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-static-files-copy": "^2.3.1",
        "parcel-plugin-svelte": "^4.0.6",
        "prettier": "^2.0.5",
        "svelte": "^3.49.0",
        "typescript": "^3.8.3"
    }
}
